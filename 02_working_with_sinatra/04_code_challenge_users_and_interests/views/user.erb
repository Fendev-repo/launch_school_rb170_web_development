<br>
<h1>Profile</h1>
<div class="row">
  <div class="col s12">
    <div class="card">
      <div class="card-image">
      </div>
      <div class="card-content">
        <h5><%= @name.capitalize %></h5>
      </div>
      <div class="card-action">
        <h5><%= @user_profile[:email] %></h5>
      </div>
      <div class="card-action">
        <h6><strong>Interests include...</strong></h6>
        <% @user_profile[:interests].each do |interest| %>
          <p class="center"><%= interest.capitalize %></p>
        <% end %>
      </div>
    </div>
  </div>
</div>